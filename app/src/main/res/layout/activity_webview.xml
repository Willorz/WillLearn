<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp">

    <TextView
        android:id="@+id/txv_webview_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="title"/>

    <TextView
        android:id="@+id/txv_webview_beginLoading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txv_webview_title"
        android:text="beginLoading"/>

    <TextView
        android:id="@+id/txv_webview_loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txv_webview_beginLoading"
        android:text="Loading"/>

    <TextView
        android:id="@+id/txv_webview_endLoading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/txv_webview_loading"
        android:text="endLoading"/>

    <WebView
        android:id="@+id/wbv_webview_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/txv_webview_endLoading"
        android:layout_marginTop="10dp"></WebView>


</RelativeLayout>
